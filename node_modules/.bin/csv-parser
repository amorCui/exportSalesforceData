#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_csv-parser@2.3.3@csv-parser/bin/csv-parser" "$@"
  ret=$?
else 
  node  "$basedir/../_csv-parser@2.3.3@csv-parser/bin/csv-parser" "$@"
  ret=$?
fi
exit $ret
